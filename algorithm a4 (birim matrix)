#include <stdio.h>
#include <stdlib.h>
int main()
{
	int matt[10][10];
	int i, j;
	for (i = 0; i < 10; i++)
	{
		for (j = 0; j < 10; j++)
		{
			if (i == j)
			{
				matt[i][j] = 1;
			}
			else

				matt[i][j] = 0;

		}
	}
	for (i = 0; i < 10; i++)
		{
			for(j=0; j<10; j++)
			{
				printf("%4d", matt[i][j]);
			}
			printf("\n");
		}
	
	return 0;
}
