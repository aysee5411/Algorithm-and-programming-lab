#include <stdio.h>
#include <stdlib.h>
int main(void)
{
	float r1, r2, r3;
	printf("enter the resistance:\n");
	scanf("%f%f", &r1, &r2);
	if (!(r1 == 0) && !(r2 == 0))
	{
		r3 = 1.0 / r1 + 1.0 / r2;
		printf("total resistance = %f ohm\n",1.0/r3);
	}
	else 
		printf("either r1 and r2 is short circuit... bye bye");
	return 0;
}
